#!/usr/bin/env bash

set -v
set +e
travis_start('uwm_simulate_deploy')


simulate_deploy() {

  blt artifact:deploy --verbose --no-interaction --dry-run --commit-msg "Automated commit by Travis CI for Build ${TRAVIS_BUILD_ID}" || true

}

echo "UWM simulate_deploy"
date

simulate_deploy || true

echo "UWM finish simulate_deploy"
date


travis_end('uwm_simulate_deploy')

set +v
set -e