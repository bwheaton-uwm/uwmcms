{
    "name": "uwmweb/uwmcms",
    "description": "Project template for Drupal 8 sites built with BLT.",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "uwmbase-theme": {
            "type": "vcs",
            "url": "https://github.com/uwmweb/uwmbase"
        },
        "ckeditor-bs-grid": {
            "type": "vcs",
            "url": "https://github.com/uwmweb/ckeditor_bs_grid"
        },
        "uwmedicineorg-theme": {
            "type": "vcs",
            "url": "https://github.com/uwmweb/uwmedicineorg_theme"
        },
        "ckeditor-templates": {
            "type": "package",
            "package": {
                "name": "ckeditor/templates",
                "version": "4.10.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/templates/releases/templates_4.10.1.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        }
    },
    "scripts": {
        "uwmcms-update-saml": [
            "sh ./patches/simplesaml/setup_simplesaml_patch.sh"
        ],
        "post-install-cmd": [
            "@uwmcms-update-saml"
        ],
        "post-update-cmd": [
        ],
        "post-create-project-cmd": [
            "blt internal:create-project:init-repo"
        ],
        "nuke": [
            "rm -rf vendor composer.lock docroot/core docroot/modules/contrib docroot/profiles/contrib docroot/themes/contrib",
            "@composer clearcache --ansi",
            "@composer install --ansi"
        ]
    },
