/**
 * Implements hook_uninstall().
 */
function uwm_res_import_uninstall() {

  db_query("DELETE FROM {config} WHERE name LIKE '%uwm_res_import%'");
  db_query("DELETE FROM {config} WHERE name LIKE '%migration.uwm_res%'");
  drupal_flush_all_caches();

}
