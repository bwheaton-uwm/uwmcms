    <!-- Sections field -->
    <div class="field field--name-field-homepage-sections">

      <!-- Patient Care -->
      <section class="homepage-section homepage-section--1 fade-in-animation">

        <div class="homepage-section__image">

          <picture>
            <!--[if IE 9]><video style="display: none;"><![endif]-->

            <source srcset="/sites/stevie/files/styles/home_section_xl_mid/public/2019-06/lucy-hwang.jpg 1x" media="(min-width: 1600px)" type="image/jpeg">
            <source srcset="/sites/stevie/files/styles/home_section_xl/public/2019-06/lucy-hwang.jpg 1x" media="(min-width: 1200px)" type="image/jpeg">
            <source srcset="/sites/stevie/files/styles/home_section_lg/public/2019-06/lucy-hwang.jpg 1x" media="(min-width: 992px)" type="image/jpeg">
            <source srcset="/sites/stevie/files/styles/home_section_md/public/2019-06/lucy-hwang.jpg 1x" media="(min-width: 768px)" type="image/jpeg">
            <source srcset="/sites/stevie/files/styles/home_section_sm/public/2019-06/lucy-hwang.jpg 1x" media="(min-width: 576px)" type="image/jpeg">
            <source srcset="/sites/stevie/files/styles/home_section_xs/public/2019-06/lucy-hwang.jpg 1x" type="image/jpeg">
            <!--[if IE 9]></video><![endif]-->

