      </picture>
      {# /RESPONSIVE IMAGE VERSION #}

      <!-- Translucent overlay for mobile -->
      <div class="hero__overlay-mobile"></div>

      <!-- Gradient overlay for sm+ -->
      <div class="hero__overlay-gradient-horizontal"></div>

    </div>

    <div class="hero__content container-xl">

      <h1 class="hero__heading">
        A Higher Degree<br>
        of Healthcare
      </h1>

      <div class="hero__ctas">

        <a class="hero__btn-cta" href="/patient-resources/make-an-appointment">Make an appointment</a>

        <a class="hero__btn-cta" href="/services/urgent-care">Find urgent care</a>

        <a class="hero__btn-cta" href="/search/providers">Find a provider</a>

        <a class="hero__btn-cta" href="/search/locations">Find a location</a>

      </div>

    </div>

  </section>
  <!-- /Hero -->


  <div class="field field--name-body">

