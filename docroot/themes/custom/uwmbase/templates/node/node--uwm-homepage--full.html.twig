        <source srcset="/sites/stevie/files/styles/hero_xl/public/2019-06/grandmother-and-granddaughter-pnw_6.11.jpg 1x" media="(min-width: 1200px)" type="image/jpeg">
        <source srcset="/sites/stevie/files/styles/hero_lg/public/2019-06/grandmother-and-granddaughter-pnw_6.11.jpg 1x" media="(min-width: 992px)" type="image/jpeg">
        <source srcset="/sites/stevie/files/styles/hero_md/public/2019-06/grandmother-and-granddaughter-pnw_6.11.jpg 1x" media="(min-width: 768px)" type="image/jpeg">
        <source srcset="/sites/stevie/files/styles/hero_sm/public/2019-06/grandmother-and-granddaughter-pnw_6.11.jpg 1x" media="(min-width: 576px)" type="image/jpeg">
        <source srcset="/sites/stevie/files/styles/hero_xs/public/2019-06/grandmother-and-granddaughter-pnw_6.11.jpg 1x" type="image/jpeg">
