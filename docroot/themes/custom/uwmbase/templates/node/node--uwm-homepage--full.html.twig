    <!-- Alert bar -->
    <div class="home-alert gradient-purple-horizontal uwm-accent-color__white">
      <div class="container-xl">
        <div class="row">
          <div class="home-alert__text col-sm-9">
            <p>Find out more about the current measles outbreak and how to protect yourself.</p>
          </div>

          <div class="home-alert__cta col-sm-3">
            <a class="btn btn-cta" href="https://www.kingcounty.gov/depts/health/communicable-diseases/disease-control/measles.aspx">Read more</a>
          </div>
        </div>
      </div>
    </div>

  </div>


  <!-- Sections -->
